{
  "name": "cha_ching",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "compile": "tsc --noEmit -p .",
    "precommit": "lint-staged",
    "prepush": "yarn test",
    "format": "yarn -s format:js && yarn -s format:ts && yarn -s format:json",
    "format:js": "prettier --write {.,**}/*.js",
    "format:json": "prettier --write {.,**}/*.json",
    "format:ts": "prettier --write **/*.{ts,tsx} && tslint --fix -p tsconfig.json",
    "lint": "yarn -s lint:ts",
    "lint:ts": "tslint -p tsconfig.json",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "react": "16.3.1",
    "react-native": "0.55.2",
    "react-native-check-box": "^2.1.0",
    "react-native-collapsible": "^0.11.0",
    "react-native-confirmation-code-input": "^1.0.4",
    "react-native-image-picker": "^0.26.7",
    "react-native-material-textfield": "^0.12.0",
    "react-native-phone-input": "^0.2.1",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^1.5.11",
    "react-navigation-redux-helpers": "^1.0.5",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0",
    "reduxsauce": "^0.7.0",
    "seamless-immutable": "^7.1.3"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/ramda": "^0.25.22",
    "@types/react": "^16.3.11",
    "@types/react-native": "^0.55.2",
    "@types/react-native-collapsible": "^0.8.2",
    "@types/react-native-scrollable-tab-view": "^0.8.0",
    "@types/react-native-vector-icons": "^4.6.0",
    "@types/react-navigation": "^1.5.5",
    "@types/react-redux": "^5.0.16",
    "@types/react-test-renderer": "^16.0.1",
    "@types/seamless-immutable": "^7.1.1",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "husky": "^0.14.3",
    "jest": "22.4.3",
    "jest-preset-typescript-react-native": "^1.2.0",
    "lint-staged": "^7.0.4",
    "prettier": "^1.12.0",
    "react-native-typescript-transformer": "^1.2.4",
    "react-test-renderer": "16.3.1",
    "solidarity": "^2.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "preset": "jest-preset-typescript-react-native",
    "setupFiles": [
      "<rootDir>/node_modules/react-native/jest/setup.js",
      "./test/setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|react-navigation)"
    ]
  },
  "lint-staged": {
    "concurrent": false,
    "subTaskConcurrency": 1,
    "linters": {
      "*.{ts,tsx}": [
        "prettier --write",
        "tslint --fix -p tsconfig.json",
        "git add"
      ]
    }
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
